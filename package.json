{
  "name": "@jackson-bayor/bayor-ui",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "dist/bayor-ui.umd.js",
  "module": "dist/bayor-ui.es.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "sh ./scripts/prebuild.sh && sh ./scripts/build.sh && sh ./scripts/migrate-dist.sh",
    "build-lib": "vite build && vue-tsc --project ./tsconfig.lib.json",
    "prebuild": "sh ./scripts/prebuild.sh",
    "changeset": "changeset",
    "version:dev": "changeset version",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/bayor-ui.es.js",
      "require": "./dist/bayor-ui.umd.js"
    }
  },
  "peerDependencies": {
    "element-plus": "^2.10.1",
    "tailwindcss": "^4.1.8",
    "vue": "^3.5.13"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "element-plus": "^2.10.1",
    "tailwindcss": "^4.1.8",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.4",
    "@chromatic-com/storybook": "^4.0.0",
    "@storybook/addon-a11y": "^9.0.8",
    "@storybook/addon-docs": "9.0.8",
    "@storybook/addon-onboarding": "9.0.8",
    "@storybook/addon-vitest": "^9.0.8",
    "@storybook/vue3-vite": "9.0.8",
    "@types/node": "^24.0.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/browser": "^3.2.3",
    "@vitest/coverage-v8": "^3.2.3",
    "@vue/tsconfig": "^0.7.0",
    "enquirer": "2.3.6",
    "playwright": "^1.52.0",
    "sass-embedded": "^1.89.2",
    "storybook": "9.0.8",
    "typescript": "~5.8.3",
    "unplugin-element-plus": "^0.10.0",
    "vite": "^6.3.5",
    "vite-plugin-lib": "^3.0.1",
    "vitest": "^3.2.3",
    "vue-tsc": "^2.2.8"
  },
  "publishConfig": {
    "access": "public"
  }
}
